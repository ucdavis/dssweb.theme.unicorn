  xmlns:faceted="http://namespaces.zope.org/faceted"
  i18n_domain="eea">

  <faceted:widget factory=".widget.Widget" />

  <browser:resource
    name="dssweb.theme.unicorn.datepicker.view.js"
    file="./view.js"
   />

  <browser:resource
    name="dssweb.theme.unicorn.datepicker.edit.js"
    file="./edit.js"
